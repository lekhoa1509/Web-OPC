    <script>
        let sidebar = document.querySelector(".sidebar");
        let sidebarBtn = document.querySelector(".sidebarBtn");
        sidebarBtn.onclick = function () {
            sidebar.classList.toggle("active");
            if (sidebar.classList.contains("active")) {
                sidebarBtn.classList.replace("bx-menu", "bx-menu-alt-right");
            } else
                sidebarBtn.classList.replace("bx-menu-alt-right", "bx-menu");
        }

        function logout() {
            window.location.href = "/home/login"
        }
        document.getElementById("TOP1").innerHTML = Response.innerHTML.g;

        $("#leftside-navigation .sub-menu > a").click(function (e) {
            $("#leftside-navigation ul ul").slideUp(), $(this).next().is(":visible") || $(this).next().slideDown(),
                e.stopPropagation()
        })
        function ChangPassword() {
            window.location.href = "/home/DoiMatKhau"
        }
    </script>
    <script>
        // JavaScript để xử lý sự kiện click và điều khiển hiển thị của danh sách và bài viết
        const toggleList = document.getElementById("toggleList");
        const contentList = document.getElementById("contentList");
        const toggleArticle = document.getElementById("toggleArticle");
        const article = document.querySelector("article");

        toggleList.addEventListener("click", function () {
            contentList.classList.toggle("collapsed");
        });

        toggleArticle.addEventListener("click", function () {
            article.classList.toggle("collapsed");
        });
    </script>
